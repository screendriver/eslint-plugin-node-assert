{
	"extends": ["config:base"],
	"labels": ["renovate", "upgrade"],
	"dependencyDashboard": false,
	"lockFileMaintenance": {
		"enabled": true,
		"automerge": true
	},
	"nvm": {
		"enabled": false
	},
	"packageRules": [
		{
			"matchPackagePatterns": ["^@enormora/eslint-config"],
			"groupName": "enormora-eslint-config"
		},
		{
			"depTypeList": ["dependencies", "devDependencies"],
			"updateTypes": ["minor", "patch"],
			"automerge": true,
			"automergeType": "pr"
		}
	]
}
